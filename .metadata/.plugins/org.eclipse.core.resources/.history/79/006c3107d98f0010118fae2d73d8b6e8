spring.application.name=OrderProject

spring.datasource.url=jdbc:postgresql://localhost:5432/StudentManagement
spring.datasource.username=postgres
spring.datasource.password=postgres123
spring.datasource.driver-class-name=org.postgresql.Driver

server.port=8000
#Setting the Context Root Path
server.servlet.context-path=/order-app

#It is the address of the Eureka Server that all Eureka Clients use to register and discover services.
eureka.client.service-url.defaultZone=http://localhost:8761/eureka/

eureka.instance.metadata-map.contextPath=/product-app
